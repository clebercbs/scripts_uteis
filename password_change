import getpass
import sys
import telnetlib


list = open(b"C:\Users\xxxxx\xxxxx\hosts.txt","r")
user = input("Entreer your telnet username : ")
password = getpass.getpass()
for item in list:
    h = item.split()
    HOST = h[0]
    tn = telnetlib.Telnet(HOST)
    tn.read_until(b"Username: ")
    tn.write(user.encode('ascii') + b"\n")
    if password:
        tn.read_until(b"Password: ")
        tn.write(password.encode('ascii') + b"\n")
        
    tn.write(b"enable\n")
    tn.write(password.encode('ascii') + b"\n")
    tn.write(b"conf t\n")
    tn.write(b"username admin password novasenha\n")
    #tn.write(b"exit\n")
    tn.write(b"enable secret novasenha\n")
    #tn.write(b"exit\n")
    tn.write(b"line vty 0 15\n")
    tn.write(b"password novasenha\n")          
    tn.write(b"exit\n")
    tn.write(b"end\n")
    tn.write(b"wr\n")
    tn.write(b"exit\n")
    print(tn.read_all())
list.close()
